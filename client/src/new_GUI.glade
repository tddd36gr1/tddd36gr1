<?xml version="1.0"?>
<interface>
  <!-- interface-requires gtk+ 2.12 -->
  <!-- interface-naming-policy project-wide -->
  <object class="GtkWindow" id="window1">
    <property name="visible">True</property>
    <property name="resizable">False</property>
    <child>
      <object class="GtkHBox" id="hbox1">
        <property name="width_request">800</property>
        <property name="height_request">480</property>
        <property name="visible">True</property>
        <child>
          <object class="GtkVButtonBox" id="main_menu">
            <property name="width_request">125</property>
            <property name="visible">True</property>
            <property name="homogeneous">True</property>
            <property name="layout_style">spread</property>
            <child>
              <object class="GtkButton" id="map_button">
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="receives_default">True</property>
                <signal name="clicked" handler="on_map_button_clicked"/>
                <child>
                  <object class="GtkImage" id="map_button_img">
                    <property name="visible">True</property>
                    <property name="pixbuf">menybilder/kompass.png</property>
                  </object>
                </child>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="fill">False</property>
                <property name="position">0</property>
              </packing>
            </child>
            <child>
              <object class="GtkButton" id="mission_button">
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="receives_default">True</property>
                <signal name="clicked" handler="on_mission_button_clicked"/>
                <child>
                  <object class="GtkImage" id="mission_button_img">
                    <property name="visible">True</property>
                    <property name="pixbuf">menybilder/uppdrag.png</property>
                  </object>
                </child>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="fill">False</property>
                <property name="position">1</property>
              </packing>
            </child>
            <child>
              <object class="GtkButton" id="phone_button">
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="receives_default">True</property>
                <signal name="clicked" handler="on_phone_button_clicked"/>
                <child>
                  <object class="GtkImage" id="phone_button_img">
                    <property name="visible">True</property>
                    <property name="pixbuf">menybilder/telefon.png</property>
                  </object>
                </child>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="fill">False</property>
                <property name="position">2</property>
              </packing>
            </child>
            <child>
              <object class="GtkButton" id="messaging_button">
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="receives_default">True</property>
                <signal name="clicked" handler="on_messaging_button_clicked"/>
                <child>
                  <object class="GtkImage" id="messaging_button_img">
                    <property name="visible">True</property>
                    <property name="pixbuf">menybilder/brev.png</property>
                  </object>
                </child>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="fill">False</property>
                <property name="position">3</property>
              </packing>
            </child>
            <child>
              <object class="GtkButton" id="off_button">
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="receives_default">True</property>
                <signal name="clicked" handler="__quit__"/>
                <child>
                  <object class="GtkImage" id="off_button_img">
                    <property name="visible">True</property>
                    <property name="pixbuf">menybilder/off.png</property>
                  </object>
                </child>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="fill">False</property>
                <property name="position">4</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="position">0</property>
          </packing>
        </child>
        <child>
          <object class="GtkNotebook" id="main_notebook">
            <property name="width_request">675</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="show_tabs">False</property>
            <property name="show_border">False</property>
            <property name="tab_border">0</property>
            <property name="tab_hborder">0</property>
            <property name="tab_vborder">0</property>
            <child>
              <object class="GtkHBox" id="map_placeholder">
                <property name="visible">True</property>
                <child>
                  <object class="GtkLabel" id="map_placeholder_label">
                    <property name="visible">True</property>
                    <property name="label" translatable="yes">INSERT MAP COMPONENT HERE</property>
                  </object>
                  <packing>
                    <property name="position">0</property>
                  </packing>
                </child>
              </object>
            </child>
            <child type="tab">
              <placeholder/>
            </child>
            <child>
              <object class="GtkNotebook" id="mission_notebook">
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="tab_pos">bottom</property>
                <child>
                  <object class="GtkVBox" id="mission_my_vbox">
                    <property name="visible">True</property>
                    <child>
                      <object class="GtkLabel" id="mission_my_title">
                        <property name="visible">True</property>
                        <property name="label" translatable="yes">Mina uppdrag</property>
                      </object>
                      <packing>
                        <property name="expand">False</property>
                        <property name="fill">False</property>
                        <property name="padding">20</property>
                        <property name="position">0</property>
                      </packing>
                    </child>
                    <child>
                      <object class="GtkHBox" id="hbox2">
                        <property name="visible">True</property>
                        <child>
                          <object class="GtkLabel" id="label2">
                            <property name="width_request">300</property>
                            <property name="visible">True</property>
                            <property name="xalign">0</property>
                            <property name="label" translatable="yes">Uppdrag</property>
                          </object>
                          <packing>
                            <property name="expand">False</property>
                            <property name="fill">False</property>
                            <property name="position">0</property>
                          </packing>
                        </child>
                        <child>
                          <object class="GtkLabel" id="label1">
                            <property name="visible">True</property>
                            <property name="xalign">0</property>
                            <property name="label" translatable="yes">Status</property>
                          </object>
                          <packing>
                            <property name="position">1</property>
                          </packing>
                        </child>
                      </object>
                      <packing>
                        <property name="expand">False</property>
                        <property name="position">1</property>
                      </packing>
                    </child>
                    <child>
                      <object class="GtkScrolledWindow" id="scrolledwindow1">
                        <property name="visible">True</property>
                        <property name="can_focus">True</property>
                        <property name="hscrollbar_policy">never</property>
                        <property name="vscrollbar_policy">automatic</property>
                        <child>
                          <object class="GtkTreeView" id="missions_my_treeview">
                            <property name="visible">True</property>
                            <property name="can_focus">True</property>
                            <property name="model">mission_my_liststore</property>
                            <property name="headers_visible">False</property>
                            <property name="search_column">0</property>
                            <signal name="row_activated" handler="on_missions_my_row_changed"/>
                            <child>
                              <object class="GtkTreeViewColumn" id="mission_my_titlecol">
                                <property name="sizing">fixed</property>
                                <property name="fixed_width">300</property>
                                <property name="title">Titel</property>
                                <property name="clickable">True</property>
                                <child>
                                  <object class="GtkCellRendererText" id="mission_my_titlecell"/>
                                  <attributes>
                                    <attribute name="text">0</attribute>
                                  </attributes>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkTreeViewColumn" id="mission_my_statuscol">
                                <property name="sizing">fixed</property>
                                <property name="fixed_width">200</property>
                                <property name="title">Status</property>
                                <property name="clickable">True</property>
                                <child>
                                  <object class="GtkCellRendererText" id="mission_my_statuscell"/>
                                  <attributes>
                                    <attribute name="text">1</attribute>
                                  </attributes>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkTreeViewColumn" id="mission_my_buttoncol">
                                <property name="title">Buttons</property>
                                <property name="clickable">True</property>
                                <child>
                                  <object class="GtkCellRendererPixbuf" id="mission_my_buttoncell">
                                    <property name="stock_id">gtk-dnd</property>
                                  </object>
                                </child>
                                <child>
                                  <object class="GtkCellRendererText" id="mission_my_buttontext">
                                    <property name="text">Mer info</property>
                                  </object>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>
                      </object>
                      <packing>
                        <property name="position">2</property>
                      </packing>
                    </child>
                  </object>
                </child>
                <child type="tab">
                  <object class="GtkLabel" id="mission_my_label">
                    <property name="visible">True</property>
                    <property name="label" translatable="yes">Mina uppdrag</property>
                  </object>
                  <packing>
                    <property name="tab_fill">False</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkLabel" id="label4">
                    <property name="visible">True</property>
                    <property name="yalign">0</property>
                    <property name="label" translatable="yes">Skapa uppdrag</property>
                    <property name="justify">center</property>
                  </object>
                  <packing>
                    <property name="position">1</property>
                  </packing>
                </child>
                <child type="tab">
                  <object class="GtkLabel" id="mission_create_label">
                    <property name="visible">True</property>
                    <property name="label" translatable="yes">Skapa uppdrag</property>
                  </object>
                  <packing>
                    <property name="position">1</property>
                    <property name="tab_fill">False</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkLabel" id="label8">
                    <property name="visible">True</property>
                    <property name="yalign">0</property>
                    <property name="label" translatable="yes">Alla uppdrag</property>
                  </object>
                  <packing>
                    <property name="position">2</property>
                  </packing>
                </child>
                <child type="tab">
                  <object class="GtkLabel" id="mission_all_label">
                    <property name="visible">True</property>
                    <property name="label" translatable="yes">Alla uppdrag</property>
                  </object>
                  <packing>
                    <property name="position">2</property>
                    <property name="tab_fill">False</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkLabel" id="label7">
                    <property name="visible">True</property>
                    <property name="yalign">0</property>
                    <property name="label" translatable="yes">Slutf&#xF6;rda uppdrag</property>
                  </object>
                  <packing>
                    <property name="position">3</property>
                  </packing>
                </child>
                <child type="tab">
                  <object class="GtkLabel" id="mission_finished_label">
                    <property name="visible">True</property>
                    <property name="label" translatable="yes">Slutf&#xF6;rda uppddrag</property>
                  </object>
                  <packing>
                    <property name="position">3</property>
                    <property name="tab_fill">False</property>
                  </packing>
                </child>
              </object>
              <packing>
                <property name="position">1</property>
              </packing>
            </child>
            <child type="tab">
              <placeholder/>
            </child>
            <child>
              <object class="GtkImage" id="image3">
                <property name="visible">True</property>
                <property name="pixbuf">trololo.jpg</property>
              </object>
              <packing>
                <property name="position">2</property>
              </packing>
            </child>
            <child type="tab">
              <placeholder/>
            </child>
            <child>
              <object class="GtkImage" id="image4">
                <property name="visible">True</property>
                <property name="pixbuf">fat.jpg</property>
              </object>
              <packing>
                <property name="position">3</property>
              </packing>
            </child>
            <child type="tab">
              <placeholder/>
            </child>
          </object>
          <packing>
            <property name="position">1</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
  <object class="GtkListStore" id="mission_my_liststore">
    <columns>
      <!-- column-name Titel -->
      <column type="gchararray"/>
      <!-- column-name status -->
      <column type="gchararray"/>
    </columns>
  </object>
</interface>
